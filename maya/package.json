{
  "name": "Maya",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android start --mode=developmentdebug --appId=com.maya.development --verbose --port=8088",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native run-android start --mode=developmentDebug --appId=com.maya.development --port=8088",
    "test": "jest",
    "postinstall": "patch-package",
    "android:stg": "react-native  run-android --mode=stagingDebug --appId=com.maya.staging --verbose --port=8088",
    "android:stg-release": "react-native run-android --mode=stagingRelease --appId=com.maya.staging",
    "android:dev": "react-native  run-android --mode=developmentDebug --appId=com.maya.development --verbose --port=8088",
    "android:dev-release": "react-native run-android --mode=developmentRelease --appId=com.maya.development ",
    "android:prod": "react-native  run-android --mode=productionDebug --appId=com.maya.production --verbose --port=8088",
    "android:prod-release": "react-native run-android --mode=productionRelease --appId=com.maya.production",
    "apk:prod": "cd android && gradlew clean && gradlew assembleProductionRelease --appId=com.maya.production",
    "apk:stg": "cd android && gradlew clean && gradlew assembleStagingRelease --appId=com.maya.staging",
    "aab": "cd android && gradlew clean && gradlew bundleDevelopmentRelease --appId=com.maya.development",
    "aab:prod": "cd android && gradlew clean && gradlew bundleProductionRelease --appId=com.maya.production",
    "aab:stg": "cd android && gradlew clean && gradlew bundleStagingRelease --appId=com.maya.staging"
  },
